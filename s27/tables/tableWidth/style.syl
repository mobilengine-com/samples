styles {
	style header {
		all {
			backgroundColor = 666666
			fontColor = f0F0f0
			fontSize = 18px
			bold = true
		}
	}
	style tableFill {
		table {
			width = fill
			columnWidth = 1
		}
	}
	style tableContent {
		table {
			width = content
			columnWidth = 100px, 50px, 100px, 50px
		}
	}
	style tableDuHalfSwt {
		table {
			width = 0.5swt
			columnWidth = 1
		}
	}
	style tableDu150px {
		table {
			width = 150px
			columnWidth = 1
		}
	}
}