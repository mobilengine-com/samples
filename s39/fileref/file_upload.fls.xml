<form id="file_upload" 
      menuName="file_upload" 
      title='file_upload' 
      platforms="web" 
      xmlns="http://schemas.mobilengine.com/fls/v2">

    <fileupload id="file" label="Upload a file (jpg/pdf):" acceptedMimetypes="application/octet-stream,image/jpeg,application/pdf"/>
    <textbox id="priority" label="Priority:" text="1" />
    
    <textview text="Uploaded files will be stored in the files reftab by an RFS." />
</form>
