server program on_report_form for form report_form
using rfs rfs_started_from_rfs;
using report report_eachtime_pdf;
{
	// start another rfs
	var rfsRun = rfs.rfs_started_from_rfs.New();
	rfsRun.SetHighPriority(true);
	rfsRun.Run();

	// start a report
	var repRun = reports.report_eachtime_pdf.New();
	repRun.HighPriority = true;
	repRun.Run();
}
