<!-- contains an image for every page of the `pdfs` table -->
<Reftab Name="blueprints" xmlns="http://schemas.mobilengine.com/reftab/v1">
    <Columns>
        <Column Name="imageMediaId" Type="Text" NotNull="true" PrimaryKey="true"/>
        <Column Name="pdfMediaId" Type="Text" NotNull="true"/> <!-- references `pdfs` -->
        <Column Name="pdfPageIndex" Type="Integer" NotNull="true"/>
        <Column Name="createDate" Type="Date" NotNull="true"/>
    </Columns>
</Reftab>