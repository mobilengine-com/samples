styles {
/* icon */
  style icon {
    all {
      label {
        borderColor = 2196F3
        backgroundColor = 90CAF9
        borderWidth { top=5px right=5px bottom=5px left=5px }
      }
	  
    }
  }

  style title4Icon {
    all {
      backgroundColor = CDCDCD
    }
  }

  style iconTop {
    all {
      label {
        iconPosition = top
      }
      control {
        iconPosition = top
      }
    }
  }
  style iconRight {
    all {
      label {
        iconPosition = right
      }
      control {
        iconPosition = right
      }
    }
  }
  style iconBottom {
    all {
      label {
        iconPosition = bottom
      }
      control {
        iconPosition = bottom
      }
    }
  }
  style iconLeft {
    all {
      label {
        iconPosition = left
