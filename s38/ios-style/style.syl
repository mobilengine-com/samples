/*cell style*/
  style table {
    table {
      borderColor = CDDC39
      backgroundColor = E6EE9C
      borderWidth { top=5px right=5px bottom=5px left=5px }

      body = table
    }
    row {
        content = table
    }
    cell {
        content = table
        borderColor = 2196F3
        backgroundColor = 90CAF9
        borderWidth { top=5px right=5px bottom=5px left=5px }
    }
    textview {
        control {
            borderColor = 4CAF50
            backgroundColor = A5D6A7
            borderWidth { top=5px right=5px bottom=5px left=5px }
        }
    }
  }

  style cellTop {
    cell {
      verticalAlign = top
    }
  }
  style cellMiddle {
    cell {
      verticalAlign = middle
    }
  }
  style cellBottom {
    cell {
      verticalAlign = bottom
    }
  }  
  style cellFill {
    cell {
      verticalAlign = fill
    }
  }
 /* label style */
  style base {
    textview {
      borderColor = CDDC39
      backgroundColor = E6EE9C
        borderWidth { top=4px right=4px bottom=4px left=4px }
        margin { top=6px right=6px bottom=6px left=6px }
        padding { top=2px right=2px bottom=2px left=2px }

      label {
        borderColor = 2196FF
        backgroundColor = 90CAF9
        borderWidth { top=4px right=4px bottom=4px left=4px }
        margin { top=6px right=6px bottom=6px left=6px }
        padding { top=2px right=2px bottom=2px left=2px }
		width = 300px
      }

      control {
        borderColor = 4CAF50
        backgroundColor = A5D6A7
        borderWidth { top=5px right=5px bottom=5px left=5px }
        width = 300px
      }
    }
  }
  
  style wfill {
    textview {
      control {
        width = fill
      }
    }
  }

  style wfit {
    textview {
      control {
        width = auto
      }
    }
  }
  style w500 {
    textview {
      control {
        width = 500px
