		<Control type="textbox" name="textbox4" label="Price" reference="SELECT @1" ref_arg="PARENT.col2" width="5em" />
		<Control type="textbox" name="textbox5" label="Quantity" reference="SELECT @1" ref_arg="PARENT.col3" width="5em" />
		
	</Control>
</Control>
</Form>
